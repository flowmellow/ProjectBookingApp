<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:converters="clr-namespace:ProjectBookingApp.Converters"
             xmlns:local="clr-namespace:ProjectBookingApp"
             x:Class="ProjectBookingApp.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
                <converters:ListContainsConverter x:Key="ListContainsConverter" />
                <converters:ListContainsToColorConverter x:Key="ListContainsToColorConverter" />            
        </ResourceDictionary>
    </Application.Resources>
</Application>
